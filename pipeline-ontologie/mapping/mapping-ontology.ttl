    rr:predicateObjectMap [
        rr:predicate rdf:type ;
        rr:objectMap [ 
            rr:template "http://ia-das.org/onto#{sub-class_Final_VD}" 
        ]
    ] ;
    # Propriétés existantes pour compatibilité SPARQL
    rr:predicateObjectMap [ 
        rr:predicate iadas:hasCategory ; 
        rr:objectMap [ rml:reference "CLASS" ] 
    ] ;
    rr:predicateObjectMap [ 
        rr:predicate iadas:measure ; 
        rr:objectMap [ rml:reference "Measure_VD" ] 
    ] .

